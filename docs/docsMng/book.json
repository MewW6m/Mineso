{
  "language": "ja",
  "plugins": [
    "hide-published-with",
    "custom-favicon",
    "-sharing",
	"collapsible-chapters",
	"expand-active-chapter",
	"livereload",
	"lunr",
	"search",
	"fontsettings",
	"uml"
  ],
  "dependencies": {
    "plantuml-encoder": "1.2.3"
  },
  "pluginsConfig": {
    "favicon": "./reference/other/image/favicon.png",
    "sharing": {
      "facebook": true,
      "twitter": true,
      "google": true,
      "weibo": false,
      "instapaper": false,
      "vk": false,
      "all": [
        "facebook",
        "twitter",
        "google",
        "weibo",
        "instapaper",
        "vk"
      ]
    },
	"uml": {
      "format": "svg",
      "charset" : "UTF-8",
      "nailgun": false
     },
	 "sunlightHighlighter": {
	  "theme": "dark",
	  "lineNumbers": true
	 },
	 "collapsible-chapters":{}
  }
}
